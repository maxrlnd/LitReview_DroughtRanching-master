file = {:Users/trsh4998/Documents/Mendeley Desktop/Ifft, Wu, Kuethe/Ifft, Wu, Kuethe - 2014 - The Impact of Pasture Insurance on Farmland Values.pdf:pdf},
journal = {Agricultural and Resource Economics Review},
keywords = {June Area Survey,farmland values,insurance},
title = {{The Impact of Pasture Insurance on Farmland Values}},
url = {https://www.researchgate.net/profile/Jennifer{\_}Ifft/publication/254384713{\_}The{\_}Impact{\_}of{\_}Pasture{\_}Insurance{\_}on{\_}Farmland{\_}Values/links/553fab100cf2320416eb50be.pdf},
