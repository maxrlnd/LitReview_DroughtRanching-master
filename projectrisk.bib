file = {:Users/trsh4998/Documents/Mendeley Desktop/An et al/An et al. - 2014 - Agent-Based Modeling in Coupled Human and Natural Systems (CHANS) Lessons from a Comparative Analysis.pdf:pdf},
isbn = {10.1080/00045608.2014.910085},
