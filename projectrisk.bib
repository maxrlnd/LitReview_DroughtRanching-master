journal = {Marketing Letters},
keywords = {decision making,regret,regret experience measure,risk aversion},
number = {4},
pages = {379--392},
title = {{The Development and Use of a Regret Experience Measure to Examine the Effects of Outcome Feedback on Regret and Subsequent Choice}},
url = {http://login.ezproxy.library.ualberta.ca/login?url=http://search.ebscohost.com/login.aspx?direct=true{\&}db=tnh{\&}AN=7595283{\&}loginpage=Login.asp{\&}site=ehost-live{\&}scope=site},
