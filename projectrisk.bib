@article{Harta,
abstract = {Stocking rate and grazing management deci-sions during drought may be the most impor-tant decisions made. The effects of these decisions go far beyond survival during the drought and greatly influence recovery follow-ing drought. No two ranches are completely identical and as such, need to make stocking rate and grazing management decisions before, during, and after a drought that are compati-ble with the goals of the operation. This paper is intended to highlight options for stocking rate and grazing management deci-sions related to drought. Supply and Demand Ranch forage supplies must be constantly evaluated to match supply with demand. Forage supply must meet or exceed forage demand or the forage base may eventually contribute to the demise of the operation. Reduction in stocking rates during severe drought pays big dividends in terms of less damage to desirable forage plants, rapid recovery of the range following drought, re-duced supplemental feeding costs, and re-duced losses to toxic plants, just to name a few. Determining the supply of forage available for livestock consumption could be one of the toughest tasks for ranchers. However, for plants to survive drought conditions, the amount of forage left ungrazed becomes as important as the amount produced. Future forage production depends on healthy plants that can survive drought and recover quickly when favorable conditions exist. Rainfall is less effective when plant and litter cover on the soil surface is reduced. Leaving stock in a pasture too long when forage supply is inade-quate may permanently reduce carrying capac-ity for both livestock and wildlife.},
author = {Hart, Charles R. and Carpenter, Bruce B.},
file = {:Users/trsh4998/Documents/Mendeley Desktop/Hart, Carpenter/Hart, Carpenter - 1999 - Stocking Rate and Grazing Management During Drought.pdf:pdf},
journal = {Texas Agricultural Extension Service},
pages = {2/99},
title = {{Stocking Rate and Grazing Management During Drought}},
volume = {RLEM No.4},
year = {1999}
}
