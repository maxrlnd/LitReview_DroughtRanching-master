number = {2},
pages = {393--399},
pmid = {18536244},
title = {{Global assessment of the non-equilibrium concept in rangelands}},
volume = {22},
year = {2012}
}
@article{Western2004,
abstract = {During the last one hundred years, Kenya's Maasailand has seen the passage of significant milestones such as the rinderpest epidemic, colonialism, agreements with the authorities, land alienation, independence, more land alienation, national park development and land conversion into agriculture. Nomadic pastoralism is a highly evolved production system, which, among the Maasai, has reached a specialised form. Pastoralists living in dryland areas have evolved a set of strategies to deal with uncertainty in rainfall and the supply of forage, and which have, until recently, allowed them to survive in those non-equilibrium areas. These environments require flexibility in range management. In traditional systems, nomadic pastoralists make decisions based on a complex set of variables. But nomadic pastoralists in Kenya and the Maasai in particular, have seen radical changes in their environment in the last one hundred years. The pace of change has accelerated in the last 30 years. Much of the change is due to the loss of resources, which has limited the choices they would normally have in the management of their livestock. These limitations become especially debilitating during times of intensified drought. Environmental changes (natural and anthropogenic) have increased the vulnerability of the Maasai pastoral populations, by reducing their resource base (land as well as livestock), and making it very difficult for them to survive on subsistence pastoralism. At present, they are particularly vulnerable to any intensifiesd droughts and are insecure in terms of poverty, food security health, socio-economic losses and sometimes to civil strife and conflicts.},
author = {Western, D and Nightingale, D},
