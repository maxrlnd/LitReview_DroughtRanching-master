keywords = {AGREEMENT,AREA,AREAS,Africa,Dryland,ENVIRONMENTAL-CHANGE,ENVIRONMENTS,Food security,Kenya,NONEQUILIBRIUM,National Park,PASTORALISTS,Poverty,SYSTEM,SYSTEMS,TIME,agriculture,anthropogenic,case study,communities,community,conflict,decision,development,drought,environment,environmental,environmental change,epidemic,food,land,limitation,livestock,management,non-equilibrium,pastoral,pastoralism,pastoralist,population,populations,production,rainfall,range,resource,resources,strategies,strategy,uncertainty,vulnerability},
